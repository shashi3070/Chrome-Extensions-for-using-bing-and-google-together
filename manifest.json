{
    "name": "Popup ",
    "manifest_version": 2,
    "version": "0.1",
    "description": "Run process on page activated by click in extension popup",
    "browser_action": {
    "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
	 "background": {
      "scripts": [ "background.js" ],
	  "persistent": false
  },

    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ]
}